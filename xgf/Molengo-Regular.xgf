<?xml version="1.0" encoding="UTF-8"?>

<!-- 
    This is the hinting instructions for the TTF version of Molengo-Regular.
-->
<xgridfit>
  <profile>
    <infile>Molengo.sfd</infile>
    <outfile>Molengo.ttf</outfile>
    <!-- Points in the twilight zone should come here -->
    <constant name="left-sidebearing" num="last + 1"/>
    <constant name="right-sidebearing" num="left-sidebearing + 1"/>
  </profile>

<!--
  Control Value Table
  Control value are named by style-type-value
  Styles for Latin script:
    default - lowercase
    "uc" - uppercase
    "onum" - old style figures
  Types for Latin script:
    default - straight glyphs or parts like 'l', 'n', etc.
    "rnd" - round glyphs or parts like 'o', 'c', etc.
-->
  <control-values>
    <control-value id="baseline" value="0"/>
    <control-value id="round-bottom" value="-18"/>
    <!-- cap-round-bottom could be different -->
    <control-value id="cap-round-bottom" value="-18"/>
    <control-value id="straight-bearing" value="164"/>
    <control-value id="cap-straight-bearing" value="180"/>
    <control-value id="round-bearing" value="61"/>
    <control-value id="cap-round-bearing" value="45"/>


    <!-- Lowercase -->
    <!-- vert-curve could be a bit larger than vert-stem -->
    <control-value id="vert-stem" value="161"/>
    <control-value id="vert-curve" value="161"/> 
    <control-value id="horiz-stem" value="128"/>
    <control-value id="horiz-curve" value="128"/>

    <!-- Uppercase -->
    <control-value id="cap-vert-stem" value="181"/>
    <control-value id="cap-vert-curve" value="192"/> 
    <control-value id="cap-horiz-stem" value="139"/>
    <control-value id="cap-horiz-curve" value="141"/>

    <!-- Old style figures -->
    <!-- onum-round-bottom could be different if necessary-->
    <!-- onum-vert-curve could be a bit larger -->
    <control-value id="onum-round-bearing" value="61"/>
    <control-value id="onum-round-bottom" value="-18"/> 
    <control-value id="onum-vert-stem" value="161"/>
    <control-value id="onum-vert-curve" value="161"/>
    <control-value id="onum-horiz-stem" value="128"/>
    <control-value id="onum-horiz-curve" value="128"/>

<!--
  x: 200/173 - thick, diag
  k: 200 - thick, thick
  v,w,y: 173/162 - diag, thin
  z: 186 - thick-diag
  thinner-diag, where was this?

-->
    <control-value id="thicker-diag" value="200"/>
    <control-value id="thick-diag" value="186"/>
    <control-value id="diag" value="174"/>
    <control-value id="thin-diag" value="162"/>
    <control-value id="thinner-diag" value="114"/>
    <control-value id="cap-n-diag" value="348"/>

    <control-value id="x-height" value="938"/>
    <control-value id="o-height" value="956"/>
    <control-value id="onum-height" value="1049"/>
    <control-value id="onum-round-height" value="1067"/>
    <control-value id="onum-tall-height" value="1305"/>
    <control-value id="onum-desc" value="-256"/>
    <control-value id="onum-round-desc" value="-350"/>
    <control-value id="ascender-height" value="1475"/>
    <control-value id="descender-bottom" value="-492"/>
    <control-value id="f-top" value="1464"/>

    <!-- Capitals -->
    <control-value id="cap-x-height" value="1391"/>
    <control-value id="cap-o-height" value="1409"/>

  <!-- glyph specific -->
    <control-value id="zero-width" value="1110"/>
    <control-value id="eight-width" value="815"/>

    <control-value id="dash-height" value="109"/>
    <control-value id="bar-width" value="109"/>
    <control-value id="dot-width" value="236"/>
    <control-value id="dot-height" value="236"/>
    <control-value id="acute-height" value="375"/>


    <control-value id="cap-o-width" value="1354"/>

    <control-value id="a-width" value="706"/>
    <control-value id="a-vert-diag-stem" value="117"/>
    <control-value id="a-vert-dip" value="90"/> <!-- a, f -->
    <control-value id="b-width" value="835"/>
    <control-value id="c-vert-dip" value="100"/>
    <control-value id="e-vert-dip" value="73"/>
    <control-value id="e-middle-bar" value="496"/> <!-- vert distance from top -->
    <control-value id="f-stem-advance" value="222"/> <!-- f, i, j -->
    <control-value id="f-height" value="1464"/>
    <control-value id="h-width" value="793"/> <!-- h, n -->
    <control-value id="k-width" value="749"/>
    <control-value id="m-half-width" value="739"/>
    <control-value id="o-width" value="890"/>
    <control-value id="s-width" value="630"/> <!-- from curve to curve, not end to end -->
    <control-value id="v-bearing" value="21"/>
    <control-value id="v-width" value="942"/>
    <control-value id="y-width" value="982"/>

  </control-values>

  <functions>
  </functions>

  <pre-program>
    <if test="pixels-per-em &lt; 9 or pixels-per-em &gt; 1024">
      <disable-instructions/>
    </if>
    <round value="round-bottom"/>
    <round value="cap-round-bottom"/>
    <round value="vert-stem"/>
    <round value="vert-curve"/>
    <round value="horiz-stem"/>
    <round value="horiz-curve"/>
    <round value="cap-vert-stem"/>
    <round value="cap-vert-curve"/>
    <round value="cap-horiz-stem"/>
    <round value="cap-horiz-curve"/>


    <control-value-delta>
      <delta-set size="1" distance="8" cv="onum-round-height"/>
      <delta-set size="3" distance="8" cv="onum-round-height"/>
      <delta-set size="5" distance="8" cv="onum-round-height"/>
      <delta-set size="1" distance="8" cv="onum-height"/>
      <delta-set size="3" distance="8" cv="onum-height"/>
      <delta-set size="5" distance="8" cv="onum-height"/>

      <delta-set size="17" distance="8" cv="dash-height"/>
      <delta-set size="18" distance="8" cv="dash-height"/>
      <delta-set size="19" distance="8" cv="dash-height"/>

      <delta-set size="5" distance="8" cv="zero-width"/>

      <delta-set size="5" distance="-8" cv="o-height"/>
      <delta-set size="16" distance="8" cv="x-height"/>
      <delta-set size="18" distance="8" cv="x-height"/>
      <delta-set size="20" distance="8" cv="x-height"/>
      <delta-set size="2" distance="-8" cv="cap-o-height"/>

      <delta-set size="7" distance="-8" cv="cap-vert-stem"/>
      <delta-set size="17" distance="8" cv="cap-vert-stem"/>
      <delta-set size="18" distance="8" cv="cap-vert-stem"/>
      <delta-set size="19" distance="8" cv="cap-vert-stem"/>
      <delta-set size="7" distance="-8" cv="cap-vert-curve"/>
      <delta-set size="17" distance="8" cv="cap-vert-curve"/>

      <delta-set size="13" distance="-8" cv="cap-horiz-stem"/>
      <delta-set size="13" distance="-8" cv="cap-horiz-curve"/>

      <delta-set size="7" distance="-8" cv="thicker-diag"/>

      <delta-set size="8" distance="8" cv="vert-stem"/>
      <delta-set size="9" distance="8" cv="vert-stem"/>
      <delta-set size="17" distance="8" cv="vert-stem"/>
      <delta-set size="18" distance="8" cv="vert-stem"/>
      <delta-set size="19" distance="8" cv="vert-stem"/>
      <delta-set size="20" distance="8" cv="vert-stem"/>
      <delta-set size="21" distance="8" cv="vert-stem"/>
      <delta-set size="22" distance="8" cv="vert-stem"/>
      <delta-set size="8" distance="8" cv="vert-curve"/>
      <delta-set size="9" distance="8" cv="vert-curve"/>
      <delta-set size="17" distance="8" cv="vert-curve"/>
      <delta-set size="18" distance="8" cv="vert-curve"/>
      <delta-set size="19" distance="8" cv="vert-curve"/>
      <delta-set size="20" distance="8" cv="vert-curve"/>
      <delta-set size="21" distance="8" cv="vert-curve"/>
      <delta-set size="22" distance="8" cv="vert-curve"/>
      <delta-set size="8" distance="8" cv="onum-vert-stem"/>
      <delta-set size="9" distance="8" cv="onum-vert-stem"/>
      <delta-set size="17" distance="8" cv="onum-vert-stem"/>
      <delta-set size="18" distance="8" cv="onum-vert-stem"/>
      <delta-set size="19" distance="8" cv="onum-vert-stem"/>
      <delta-set size="20" distance="8" cv="onum-vert-stem"/>
      <delta-set size="21" distance="8" cv="onum-vert-stem"/>
      <delta-set size="22" distance="8" cv="onum-vert-stem"/>
      <delta-set size="8" distance="8" cv="onum-vert-curve"/>
      <delta-set size="9" distance="8" cv="onum-vert-curve"/>
      <delta-set size="17" distance="8" cv="onum-vert-curve"/>
      <delta-set size="18" distance="8" cv="onum-vert-curve"/>
      <delta-set size="19" distance="8" cv="onum-vert-curve"/>
      <delta-set size="20" distance="8" cv="onum-vert-curve"/>
      <delta-set size="21" distance="8" cv="onum-vert-curve"/>
      <delta-set size="22" distance="8" cv="onum-vert-curve"/>

      <delta-set size="7" distance="-8" cv="a-width"/>
      <delta-set size="7" distance="-8" cv="b-width"/>
      <delta-set size="4" distance="-8" cv="dot-width"/>
      <delta-set size="5" distance="-8" cv="dot-width"/>
      <delta-set size="6" distance="-8" cv="dot-width"/>
      <delta-set size="7" distance="-8" cv="dot-width"/>
      <delta-set size="13" distance="-8" cv="dot-width"/>
      <delta-set size="14" distance="-8" cv="dot-width"/>
      <delta-set size="15" distance="-8" cv="dot-width"/>
      <delta-set size="16" distance="-8" cv="dot-width"/>


<!--
      <delta-set size="9" distance="8" cv="horiz-stem"/>
      <delta-set size="9" distance="8" cv="horiz-curve"/>
-->

    </control-value-delta>
  </pre-program>

  <glyph ps-name="exclam">
    <declarations>
      <constant name="top-left" num="0"/>
      <constant name="bottom-left" num="3"/>
      <constant name="top-right" num="1"/>
      <constant name="bottom-right" num="2"/>
      <constant name="dot-left" num="4"/>
      <constant name="dot-top" num="7"/>
      <constant name="dot-right" num="10"/>
      <constant name="dot-bottom" num="13"/>
      <constant name="last" num="11"/>      
    </declarations>
    <with-vectors axis="y">
      <move distance="round-bottom">
        <point num="dot-bottom"/>
        <move distance="dot-height">
          <point num="dot-top"/>
        </move>
      </move>
      <move distance="cap-x-height">
        <point num="top-left"/>
      </move>
      <move>
        <reference>
          <point num="dot-top"/>
        </reference>
        <point num="bottom-left"/>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="dot-left"/>
        <align>
          <point num="top-left"/>
        </align>
        <move distance="dot-width">
          <point num="dot-right"/>
          <align>
            <point num="top-right"/>
          </align>
        </move>
      </move>
      <if test="pixels-per-em &lt; 17">
        <align>
          <reference>
            <point num="top-right"/>
          </reference>
          <point num="bottom-right"/>
        </align>
        <align>
          <reference>
            <point num="top-left"/>
          </reference>
          <point num="bottom-left"/>
        </align>
      </if>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="quotedbl">
    <declarations>
      <constant name="quote-first" num="0"/>
      <constant name="quote-second" num="4"/>
      <constant name="right-sidebearing" num="9"/>
    </declarations>
    <with-vectors axis="y">
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="quote-first"/>
        </reference>
        <point num="quote-second + 2"/>
      </move>  
    </with-vectors>
    <interpolate-untouched-points/>
    <move>
      <reference>
        <point num="quote-second"/>
      </reference>
      <point num="right-sidebearing"/>
    </move>
  </glyph>

  <glyph ps-name="quotesingle">
    <declarations>
      <constant name="last" num="3"/>
      <constant name="bottom-left" num="2"/>
      <constant name="bottom-right" num="1"/>
      <constant name="top-left" num="3"/>
      <constant name="top-right" num="0"/>
    </declarations>
    <with-vectors axis="y">
      <move>
        <point num="top-left"/>
        <move>
          <point num="bottom-left"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="bottom-left"/>
        <align>
          <point num="top-left"/>
        </align>
        <move>
          <point num="bottom-right"/>
        </move>
      </move>  
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="parenleft">
    <declarations>
      <constant name="last" num="21"/>
      <constant name="curve-left" num="11"/>
      <constant name="curve-right" num="0"/>
      <constant name="bottom-left" num="6"/>
      <constant name="bottom-right" num="5"/>
      <constant name="top-left" num="16"/>
      <constant name="top-right" num="17"/>
    </declarations>
    <with-vectors axis="y">
      <move distance="ascender-height">
        <point num="top-left"/>
      </move>
      <move distance="descender-bottom">
        <point num="bottom-left"/>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="curve-left"/>
        <move distance="vert-curve">
          <point num="curve-right"/>
          <move>
            <point num="right-sidebearing"/>
          </move>
        </move>
        <move>
          <point num="bottom-left"/>
          <align>
            <point num="top-left"/>
          </align>
          <move>
            <point num="bottom-right"/>
            <align>
              <point num="top-right"/>
            </align>
          </move>
        </move>
      </move>  
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="parenright">
    <declarations>
      <constant name="last" num="21"/>
      <constant name="curve-right" num="11"/>
      <constant name="curve-left" num="0"/>
      <constant name="top-right" num="6"/>
      <constant name="top-left" num="5"/>
      <constant name="bottom-right" num="16"/>
      <constant name="bottom-left" num="17"/>
    </declarations>
    <with-vectors axis="y">
      <move distance="ascender-height">
        <point num="top-left"/>
      </move>
      <move distance="descender-bottom">
        <point num="bottom-left"/>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="right-sidebearing"/>
        </reference>
        <point num="curve-right"/>
        <move distance="vert-curve">
          <point num="curve-left"/>
          <move>
            <point num="left-sidebearing"/>
          </move>
        </move>
        <move>
          <point num="bottom-right"/>
          <align>
            <point num="top-right"/>
          </align>
          <move>
            <point num="bottom-left"/>
            <align>
              <point num="top-left"/>
            </align>
          </move>
        </move>
      </move>  
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="plus">
    <declarations>
      <constant name="left-top" num="8"/>
      <constant name="left-bottom" num="7"/>
      <constant name="top-right" num="11"/>
      <constant name="top-left" num="10"/>
      <constant name="right-bottom" num="2"/>
      <constant name="right-top" num="1"/>
      <constant name="bottom-left" num="5"/>
      <constant name="bottom-right" num="4"/>
      <constant name="last" num="11"/>      
    </declarations>
    <with-vectors axis="y">
      <mdap>
        <point num="bottom-left"/>
      </mdap>
      <move>
        <point num="right-bottom"/>
        <align>
          <point num="left-bottom"/>
        </align>
        <move distance="dash-height">
          <point num="left-top"/>
          <align>
            <point num="right-top"/>
          </align>
        </move>
        <move>
          <point num="top-left"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left-top"/>
        <move>
          <point num="top-left"/>
          <align>
            <point num="bottom-left"/>
          </align>
          <move distance="dash-height">
            <point num="top-right"/>
            <align>
              <point num="bottom-right"/>
            </align>
            <move>
              <point num="right-top"/>
              <move>
                <point num="right-sidebearing"/>
              </move>
            </move>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="comma">
    <declarations>
      <constant name="bottom" num="16"/>
      <constant name="bottom-over" num="17"/>
      <constant name="crotch" num="0"/>
      <constant name="top" num="8"/>
      <constant name="left" num="5"/>
      <constant name="right" num="13"/>
      <constant name="last" num="18"/>
    </declarations>
    <with-vectors axis="y">
      <move distance="dot-height">
        <point num="top"/>
      </move>
      <move>
        <point num="bottom"/>
        <move min-distance="no" round="no">
          <point num="bottom-over"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <point num="left"/>
        <move distance="dot-width">
          <point num="right"/>
          <delta>
            <delta-set size="4" distance="-8"/>
            <delta-set size="5" distance="-8"/>
            <delta-set size="6" distance="-8"/>
            <delta-set size="7" distance="-8"/>
          </delta>
          <move>
            <point num="right-sidebearing"/>
          </move>
        </move>
      </move>
      <interpolate>
        <reference>
          <point num="right"/>
          <point num="left"/>
        </reference>
          <point num="crotch"/>
      </interpolate>
      <if test="pixels-per-em &lt; 64">
        <move>
          <reference>
            <point num="right"/>
          </reference>
          <point num="bottom"/>
          <if test="pixels-per-em &lt; 16">
            <move>
              <point num="bottom-over"/>
            </move>
          </if>
        </move>
      </if>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="hyphen">
    <declarations>
      <constant name="left-top" num="2"/>
      <constant name="right-bottom" num="0"/>
      <constant name="last" num="3"/>      
    </declarations>
    <with-vectors axis="y">
      <move>
        <point num="right-bottom"/>
        <move distance="dash-height">
          <point num="left-top"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left-top"/>
        <move>
          <point num="right-bottom"/>
          <move>
            <point num="right-sidebearing"/>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="period">
    <declarations>
      <constant name="bottom" num="9"/>
      <constant name="top" num="3"/>
      <constant name="left" num="0"/>
      <constant name="right" num="6"/>
      <constant name="last" num="11"/>
    </declarations>
    <with-vectors axis="y">
      <move distance="round-bottom">
        <point num="bottom"/>
        <move distance="dot-height">
          <point num="top"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left"/>
        <move distance="dot-width">
          <point num="right"/>
          <move>
            <point num="right-sidebearing"/>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="slash">
    <declarations>
      <constant name="top-left" num="0"/>
      <constant name="top-right" num="1"/>
      <constant name="base-left" num="3"/>
      <constant name="base-right" num="2"/>
      <constant name="last" num="3"/>
    </declarations>
    <with-vectors axis="y">
      <move distance="baseline">
        <point num="base-left"/>
        <move distance="ascender-height">
          <point num="top-right"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="base-left"/>
        <move>
          <point num="base-right"/>
        </move>
        <move>
          <point num="top-left"/>
          <move>
            <point num="top-right"/>
            <move>
              <point num="right-sidebearing"/>
            </move>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <glyph ps-name="zero">
    <declarations>
      <constant name="last" num="47"/>
      <constant name="base" num="36"/>
      <constant name="base-over" num="0"/>
      <constant name="top" num="24"/>
      <constant name="top-under" num="12"/>
      <constant name="left-stem-left" num="42"/>
      <constant name="left-stem-right" num="18"/>
      <constant name="right-stem-right" num="30"/>
      <constant name="right-stem-left" num="6"/>
    </declarations>
    <with-vectors axis="y">
      <move distance="onum-round-bottom" round="no" cut-in="no">
        <point num="base"/>
        <move distance="onum-horiz-curve">
          <point num="base-over"/>
        </move>
      </move>
      <move distance="onum-round-height">
        <point num="top"/>
        <move distance="onum-horiz-curve">
          <point num="top-under"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move distance="onum-round-bearing" min-distance="no">
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="left-stem-left"/>
        <move distance="onum-vert-curve">
          <point num="left-stem-right"/>
          <move distance="zero-width">
            <point num="right-stem-right"/>
<delta>
	  <delta-set size="0" distance="-8"/>
	  <delta-set size="2" distance="-8"/>
	  <delta-set size="3" distance="-8"/>
  	<delta-set size="4" distance="-8"/>
	  <delta-set size="7" distance="-8"/>
</delta>
              <move distance="onum-vert-curve">
                <point num="right-stem-left"/>
              </move>
              <move distance="onum-round-bearing">
                <point num="right-sidebearing"/>
              </move>
          </move>
        </move>
      </move>  
    </with-vectors>
    <interpolate-untouched-points/>
  </glyph>

  <!-- "1" -->
  <glyph ps-name="one">
    <declarations>
      <constant name="last" num="7"/>
      <constant name="base" num="1"/> <!-- base-right -->
      <constant name="crotch-over" num="6"/>
      <constant name="crotch" num="3"/>
      <constant name="arm-bottom" num="4"/>
      <constant name="arm-top" num="5"/>
