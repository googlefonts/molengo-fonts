  <glyph ps-name="dotbelowcomb">
    <declarations>
      <constant name="bottom" num="9"/>
      <constant name="top" num="3"/>
      <constant name="left" num="0"/>
      <constant name="right" num="6"/>
      <constant name="last" num="13"/>
      <constant name="anchor-mark-below" num="13"/>
      <constant name="anchor-mkmk-below-base" num="12"/>
    </declarations>
    <with-vectors axis="y">
      <move>
        <point num="bottom"/>
        <move distance="dot-height">
          <point num="top"/>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move>
        <point num="left"/>
        <delta>
          <delta-set size="5" distance="8"/>
          <delta-set size="6" distance="8"/>
          <delta-set size="7" distance="8"/>
          <delta-set size="17" distance="8"/>
          <delta-set size="18" distance="8"/>
          <delta-set size="19" distance="8"/>
          <delta-set size="20" distance="8"/>
        </delta>
        <move distance="dot-width">
          <point num="right"/>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
<!-- anchors -->
    <with-vectors axis="y">
      <move distance="baseline">
        <point num="anchor-mark-below"/>
        <delta>
          <delta-set size="0" distance="8"/>
          <delta-set size="1" distance="8"/>
          <delta-set size="4" distance="8"/>
          <delta-set size="5" distance="8"/>
          <delta-set size="6" distance="8"/>
          <delta-set size="7" distance="8"/>
        </delta>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <align>
        <reference>
          <point num="top"/>
        </reference>
        <point num="anchor-mark-below"/>
        <point num="anchor-mkmk-below-base"/>
      </align>
    </with-vectors>
  </glyph>

  <!-- "Ɑ" -->
  <glyph ps-name="uni2C6D">
    <declarations>
      <constant name="base" num="19"/>
      <constant name="base-over" num="16"/>
      <constant name="top" num="31"/>
      <constant name="top-under" num="4"/>
      <constant name="stem-base-right" num="34"/>
      <constant name="stem-base-left" num="35"/>
      <constant name="stem-left" num="1"/>
      <constant name="stem-right" num="33"/>
      <constant name="crotch-over" num="0"/>
      <constant name="crotch" num="36"/>
       <constant name="curve-left" num="25"/>
       <constant name="curve-right" num="10"/>
      <constant name="last" num="42"/>
      <constant name="anchor-mark-above" num="40"/>
      <constant name="anchor-topright" num="41"/>
      <constant name="anchor-mark-below-cedilla" num="39"/>
      <constant name="anchor-mark-ogonek" num="42"/>
    </declarations>
    <with-vectors axis="y">
      <move distance="baseline">
        <point num="stem-base-right"/>
      </move>
      <move distance="cap-round-bottom" round="no" cut-in="no">
        <point num="base"/>
        <move distance="cap-horiz-curve">
          <point num="base-over"/>
        </move>
      </move>
      <move distance="cap-o-height">
        <point num="top"/>
        <move distance="cap-horiz-curve">
          <point num="top-under"/>
          <interpolate>
            <reference>
              <point num="top-under"/>
              <point num="base-over"/>
            </reference>
            <point num="crotch"/>
            <point num="crotch-over"/>
          </interpolate>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move distance="cap-round-bearing">
        <reference>
          <point num="left-sidebearing"/>
        </reference>
        <point num="curve-left"/>
        <move distance="cap-vert-curve">
          <point num="curve-right"/>
        </move>
        <move>
          <point num="stem-base-right"/>
          <move distance="cap-vert-stem">
            <point num="stem-base-left"/>
            <align>
              <point num="crotch-over"/>
            </align>
          </move>
          <move distance="cap-straight-bearing" min-distance="no">
            <point num="right-sidebearing"/>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
<!-- anchors -->
    <with-vectors axis="y">
      <move distance="cap-o-height">
        <point num="anchor-mark-above"/>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <interpolate>
        <reference>
          <point num="curve-left"/>
          <point num="stem-right"/>
        </reference>
        <point num="anchor-mark-above"/>
        <point num="anchor-mark-below-cedilla"/>
      </interpolate>
      <interpolate>
        <reference>
          <point num="stem-base-left"/>
          <point num="stem-base-right"/>
        </reference>
        <point num="anchor-mark-ogonek"/>
      </interpolate>
    </with-vectors>
  </glyph>

  <!-- "ɨ" dotless -->
  <glyph ps-name="uni0268.dotless">
    <declarations>
      <constant name="base" num="10"/> <!-- base-left -->
      <constant name="arm-bottom" num="1"/>
      <constant name="arm-top-left" num="3"/>
      <constant name="top-right" num="4"/>
      <constant name="bar-left" num="13"/> <!-- top -->
      <constant name="bar-left-bottom" num="11"/> <!-- on stem -->
      <constant name="bar-right" num="6"/> <!-- top -->
      <constant name="bar-right-bottom" num="8"/> <!-- on stem -->
      <constant name="last" num="16"/>
      <constant name="anchor-mark-above" num="6"/>
      <constant name="anchor-topright" num="7"/>
      <constant name="anchor-mark-below" num="8"/>
    </declarations>
    <with-vectors axis="y">
      <mdap>
        <point num="base"/>
      </mdap>
      <move distance="x-height">
        <point num="arm-top-left"/>
        <move distance="horiz-stem">
          <point num="arm-bottom"/>
        </move>
        <move>
          <point num="bar-left"/>
          <align>
            <point num="bar-right"/>
          </align>
          <move distance="dash-height">
            <point num="bar-left-bottom"/>
            <align>
              <point num="bar-right-bottom"/>
            </align>
          </move>
        </move>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <move distance="f-stem-advance">
        <point num="base"/>
        <align>
          <point num="arm-bottom"/>
        </align>
        <move>
          <point num="arm-top-left"/>
          <align>
            <point num="bar-left"/>
          </align>
        </move>
        <move distance="vert-stem">
          <point num="top-right"/>
          <align>
            <point num="bar-right-bottom"/>
          </align>
          <move>
            <point num="bar-right"/>
            <move distance="straight-bearing">
              <point num="right-sidebearing"/>
            </move>
          </move>
        </move>
      </move>
    </with-vectors>
    <interpolate-untouched-points/>
<!-- anchors -->
    <with-vectors axis="y">
      <move distance="o-height">
        <point num="anchor-mark-above"/>
      </move>
    </with-vectors>
    <with-vectors axis="x">
      <interpolate>
        <reference>
          <point num="top-right"/>
          <point num="arm-bottom"/>
        </reference>
        <point num="anchor-mark-above"/>
        <point num="anchor-mark-below"/>
      </interpolate>
    </with-vectors>
  </glyph>

